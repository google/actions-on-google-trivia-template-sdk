{
  "name": "trivia-quiz-webhook",
  "version": "0.0.0",
  "private": true,
  "description": "Trivia Quiz Webhook",
  "license": "Apache Version 2.0",
  "author": "Google Inc.",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:check": "eslint --print-config . | eslint-config-prettier-check",
    "doc": "jsdoc -c jsdoc.conf.json",
    "doc:clean": "rm -rf doc && npm run doc",
    "doc:serve": "npm run doc:clean && live-server doc",
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "mocha '**/__tests__/*.test.js' -R spec",
    "coverage": "nyc npm run test"
  },
  "dependencies": {
    "@assistant/conversation": "^3.1.0",
    "@google-cloud/logging-winston": "^3.0.6",
    "firebase-admin": "^8.0.0",
    "firebase-functions": "^3.10.0",
    "joi": "^14.3.1",
    "moment-timezone": "^0.5.21",
    "sprintf-js": "^1.1.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.6.1",
    "eslint-config-google": "^0.9.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "jsdoc": "^3.6.3",
    "mocha": "^8.1.1",
    "nyc": "^15.0.0",
    "prettier": "1.15.3",
    "rewire": "^4.0.1",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0"
  }
}
